10. Insert + Update with COMMIT and ROLLBACK
INSERT INTO EMPLOYEES VALUES (106, 'Frank', 4);
INSERT INTO EMPLOYEES VALUES (107, 'Grace', 2);
COMMIT;

UPDATE EMPLOYEES SET name = 'Franklin' WHERE emp_id = 106;
COMMIT;

INSERT INTO EMPLOYEES VALUES (108, 'Helen', 3);
INSERT INTO EMPLOYEES VALUES (109, 'Ian', 1);

ROLLBACK;

INSERT INTO EMPLOYEES VALUES (108, 'Helen', 3);
COMMIT;
